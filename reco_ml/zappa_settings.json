{
    "reco-dev": {
        "app_function": "recommendation_ml.app",
        "aws_region": "ap-northeast-1",
        "profile_name": "default",
        "project_name": "tj",
        "runtime": "python3.11",
        "s3_bucket": "tj-zappa-serverless",
        "include": ["models/*"],
        "parameter_depth": 1,
        "layers": [
          "arn:aws:lambda:ap-northeast-1:501843401636:layer:onnxruntime-py311:1"
        ],
        "exclude_packages": ["numpy", "numpy*", "onnxruntime", "onnxruntime*", "joblib", "threadpoolctl"],
        "exclude": ["*/numpy*", "*/onnxruntime*", "*/joblib*", "*/threadpoolctl*"],
        "environment_variables": {
          "JOBLIB_TEMP_FOLDER": "/tmp",
          "JOBLIB_MULTIPROCESSING": "0"
        }
    }
}
